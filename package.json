{
	"name": "postonaut",
	"displayName": "Postonaut",
	"publisher": "zykosince94",
	"description": "REST API Client with GUI",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.openPostonaut"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.openPostonaut",
				"title": "Open Postonaut"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile && npm run copyHtml",
		"copyHtml": "copyfiles -u 1 src/extension.html out",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
